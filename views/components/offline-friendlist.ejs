<% n = NAME.length %>

<% for (var i = 0; i < n; i++) { %>
  <a href="#" class="d-flex align-items-center fri-acc">
    <div class="flex-shrink-0">
      <% gen = NAME[i].Gender    %>
      <% if(gen == "male"){    %>
      <% K = 'boy'   %>
      <% }else if(gen == "female"){    %>
      <% K = 'girl'   %>
      <% }else if(gen == "girlunknown"){    %>
      <% K = 'conf'+ Math.floor(Math.random() * 3)   %>
      <% }else{   %>
      <% K = 'conf'+ Math.floor((Math.random() * 5)+3)  %>
      <% }  %>



      <img class="img-fluid" src="/images/<%- K %>.png" id="userImage" alt="user img">
    </div>
    <div class="flex-grow-1 ms-3">
      <h3><%- NAME[i].Name %></h3>
      <p>ID : <%- NAME[i].ID %></p>
    </div>
  </a>
<% } %>

  </div>